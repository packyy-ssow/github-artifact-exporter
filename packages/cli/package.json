{
  "name": "@github/github-artifact-exporter-cli",
  "description": "Exporter for GitHub",
  "version": "3.0.0",
  "author": "Jason Macgowan",
  "bin": {
    "github-artifacts-exporter": "./bin/run"
  },
  "bugs": "https://github.com/github/github-artifact-exporter/issues",
  "dependencies": {
    "@github/github-artifact-exporter-core": "^2.0.2",
    "@oclif/core": "^3.26.0",
    "@octokit/rest": "^20.0.2",
    "@types/dateformat": "^5.0.0",
    "@types/debug": "^4.1.12",
    "@types/dot-object": "^2.1.4",
    "@types/luxon": "^3.4.0",
    "@types/progress": "^2.0.7",
    "dateformat": "^5.0.3",
    "debug": "^4.3.4",
    "dot-object": "^2.1.5",
    "jsonexport": "^3.2.0",
    "luxon": "^3.4.4",
    "progress": "^2.0.3",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.8",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.17",
    "chai": "^4.4.1",
    "globby": "^14.0.1",
    "mocha": "^10.3.0",
    "nock": "^13.5.1",
    "nyc": "^15.1.0",
    "oclif": "^4.4.8",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/github/github-artifact-exporter",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "github-artifacts-exporter",
    "dirname": "github-artifacts-exporter",
    "commands": "./lib/commands",
    "plugins": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github/github-artifact-exporter.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "pkg:linux": "npm run prepack && pkg package.json --target node18-linux-x64 --out-path ./dist && npm run postpack",
    "pkg:macos": "npm run prepack && pkg package.json --target node18-macos-x64 --out-path ./dist && npm run postpack",
    "pkg:windows": "npm run prepack && pkg package.json --target node18-win-x64 --out-path ./dist && npm run postpack",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif manifest && oclif readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "build": "npm run prepack"
  },
  "types": "lib/index.d.ts",
  "pkg": {
    "scripts": "./lib/**/*.js",
    "assets": [
      "ca/*.pem",
      "package.json"
    ]
  },
  "gitHead": "bebf4402de364eb82675f1ea2bf90085804881ee"
}
