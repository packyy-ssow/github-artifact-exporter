{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["ES2020"],
    "target": "es2020",
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true
  }
}
